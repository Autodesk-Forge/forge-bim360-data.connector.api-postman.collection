						{
							"name": "Create One_Time Request",
							"event": [
								{
									"listen": "test",
									"script": {
										"id": "d642ca35-1ec1-4f8d-8fcd-a4862bfac698",
										"exec": [
											"pm.test(\"Create One_Time Request- status code is 200\", function () {",
											"    pm.response.to.have.status(200);",
											"});",
											"",
											"pm.test(\"find request id\", function () {",
											"    var body = JSON.parse(responseBody);",
											"    pm.expect(body).not.eq(undefined);",
											"    pm.environment.set(\"request_id\", body.id);",
											"",
											"    console.log(body);",
											"}); "
										],
										"type": "text/javascript"
									}
								},
								{
									"listen": "prerequest",
									"script": {
										"id": "3ef3d625-0705-4321-a6e5-244e117d9462",
										"exec": [
											"var d = new Date()",
											"var current_time = d.getFullYear() ",
											"                +\"-\" + ((\"0\" + (d.getMonth()+1)).slice(-2) )",
											"                + \"-\" +  (\"0\" + d.getDate()).slice(-2) ",
											"                + \"T\" +(\"0\" + d.getHours()).slice(-2) ",
											"                + \":\" + (\"0\" + d.getMinutes()).slice(-2) ",
											"                + \":\" + (\"0\" + d.getSeconds()).slice(-2) ",
											"                + \".000Z\"",
											"pm.environment.set(\"current_time\", current_time);",
											"",
											"d.setMinutes(d.getMinutes()+2) // start execute after 2 minutes",
											"",
											"var effective_time = d.getFullYear() ",
											"                +\"-\" + ((\"0\" + (d.getMonth()+1)).slice(-2) )",
											"                + \"-\" +  (\"0\" + d.getDate()).slice(-2) ",
											"                + \"T\" +(\"0\" + d.getHours()).slice(-2) ",
											"                + \":\" + (\"0\" + d.getMinutes()).slice(-2)",
											"                + \":\" + (\"0\" + d.getSeconds()).slice(-2) ",
											"                + \".000Z\"",
											"pm.environment.set(\"effective_time\", effective_time);",
											"",
											""
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "Authorization",
										"type": "text",
										"value": "Bearer {{access_token}}",
										"disabled": true
									},
									{
										"key": "Content-Type",
										"type": "text",
										"value": "application/json"
									}
								],
								"body": {
									"mode": "raw",
									"raw": "{\r\n    \"description\":\"Test by API - {{current_time}}\",\r\n    \"isActive\": true, \r\n    \"scheduleInterval\": \"ONE_TIME\",\r\n    \"serviceGroups\": [ \r\n        \"issues\" \r\n    ],  \r\n    \"effectiveFrom\": \"{{effective_time}}\",\r\n    \"callbackUrl\": \"https://enm6413q7gun.x.pipedream.net/\"\r\n}"
								},
